<launch>
  <node pkg="rbx_vision" name="good_features" type="good_features.py" output="screen">

    <remap from="input_rgb_image" to="/camera/rgb/image_color" />
    
    <rosparam>
       maxCorners: 200
       qualityLevel: 0.01
       minDistance: 7
       blockSize: 10
       useHarrisDetector: False
       k: 0.04
       feature_size: 1
       show_text: True
    </rosparam>
    
  </node>
</launch>
